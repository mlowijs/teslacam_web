{
    "compilerOptions": {
        "outDir": "dist/",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "jsx": "react",
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6"
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ]
}